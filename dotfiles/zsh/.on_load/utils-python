######## Python ########
#######################################################################################

ixtall_py() {
	arr=("$@")
	for i in "${arr[@]}"
		do
			echo "installing ${LightBlue}$i"
			pip3 install $i --extra-index-url=https://__token__:${gitlab_py_package_token}@gitlab.com/api/v4/projects/30651850/packages/pypi/simple
		done
}
